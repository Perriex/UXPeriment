{
  "manifest_version": 3,
  "name": "BIPA",
  "description": "Browser intelligent personal assistant",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["startup.js", "js/dom/add-lazy-loading-to-media.js"]
    },
    {
      "run_at": "document_end",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/dom/view-recent-click.js", "js/dom/regex-search.js"]
    }
  ],
  "browser_action": {
    "default_popup": "html/index.html",
    "default_title": "Features"
  },
  "permissions": ["activeTab"]
}
