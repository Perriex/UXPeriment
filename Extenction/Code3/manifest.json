{
    "manifest_version": 2,
    "name": "inject",
    "description": "experiment",
    "version": "1.0",
    "web_accessible_resources": ["js/**/*"],
    "content_scripts": [
      {
        "run_at": "document_end",
        "matches": ["http://*/*", "https://*/*"],
        "js": ["test.js"]
      }
    ]
  }